#include <iostream>
#include <map>
#include <string>
#include<fstream>
using namespace std;

ifstream in("input.txt");

int main()
{
	map <string, int> mp;
	string s;
	int i = 1;
	while (in >> s)
	{
		int i = 1;
		if (isdigit(s[0]))
		{
			if (mp.find(s) != mp.end() && s[0] != ' ')
			{
				mp[s] = mp[s] + 1;
			}
			else
			{
				mp[s] = i;
			}
		}
		else
		{
			mp[s] = i;
		}
	}
	map <string, int> ::iterator it = mp.begin();
	string s1;
	for (; it != mp.end(); it++)
	{
		s1 = it->first;
		if (isdigit(s1[0]))
		{
			cout << it->first << ' ' << it->second << endl;
		}
	}
	in.close();
}
