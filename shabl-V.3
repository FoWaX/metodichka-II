#include <iostream>
using namespace std;

template <typename T> void vvod(T n,T m, T** arr)
{
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
		{
			cin >> arr[i][j];
		}
	}
}

template <typename T> void vivod(T n,T m, T** arr)
{
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
		{
			cout << arr[i][j] << ' ';
		}
		cout << endl;
	}
}

template <typename T> void resh(T a, T b, T n,T m, T** arr)
{
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
		{
			if ((arr[i][j] <= b) && (arr[i][j] >= a))
			{
				arr[i][j] = 0;
			}
		}
	}
}

int main()
{
	int a, b;
	cin >> a >> b;
	int n, m;
	cin >> n >> m;
	int** arr = new int* [n];
	for (int i = 0; i < n; i++)
	{
		arr[i] = new int[m];
	}
	vvod(n, m, arr);
	resh(a, b, n, m, arr);
	vivod(n, m, arr);
	delete[] arr;
}
